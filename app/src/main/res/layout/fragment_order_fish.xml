<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Orders.Order_invertebrates">

    <!-- TODO: Update blank fragment layout -->

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/black"
        tools:context=".Order.InvertebratesActivity">


        <androidx.gridlayout.widget.GridLayout

            android:layout_width="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_margin="10dp"
            android:background="@color/black"
            android:layout_height="0dp"
            app:columnCount="2"
            app:rowCount="3">

            <androidx.cardview.widget.CardView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_margin="5dp"
                app:cardElevation="0dp"

                app:cardCornerRadius="15dp"
                app:layout_rowWeight="1"
                app:layout_columnWeight="1">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/black"
                    >

                    <ImageView

                        android:id="@+id/ivCachep"
                        android:layout_width="120dp"
                        android:layout_height="150dp"

                        android:layout_centerInParent="true"
                        android:src="@drawable/cachep">

                    </ImageView>



                </RelativeLayout>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_height="0dp"
                android:layout_width="0dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="0dp"
                android:layout_margin="5dp"
                app:layout_rowWeight="1"
                app:layout_columnWeight="1">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/black"
                    >

                    <ImageView
                        android:id="@+id/ivCahoi"
                        android:layout_width="120dp"
                        android:layout_height="150dp"
                        android:layout_centerInParent="true"
                        android:src="@drawable/cahoi">

                    </ImageView>

                </RelativeLayout>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_height="0dp"
                android:layout_width="0dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="0dp"
                android:layout_margin="5dp"
                app:layout_rowWeight="1"
                app:layout_columnWeight="1">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/black"
                    >

                    <ImageView
                        android:id="@+id/ivCabien"
                        android:layout_width="120dp"
                        android:layout_height="150dp"
                        android:layout_centerInParent="true"
                        android:src="@drawable/cabien">

                    </ImageView>

                </RelativeLayout>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_height="0dp"
                android:layout_width="0dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="0dp"
                android:layout_margin="5dp"
                app:layout_rowWeight="1"
                app:layout_columnWeight="1">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/black"
                    >

                    <ImageView
                        android:id="@+id/ivCadachon"
                        android:layout_width="120dp"
                        android:layout_height="150dp"
                        android:layout_centerInParent="true"
                        android:src="@drawable/cadachon">

                    </ImageView>

                </RelativeLayout>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_height="0dp"
                android:layout_width="0dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="0dp"
                android:layout_margin="5dp"
                app:layout_rowWeight="1"
                app:layout_columnWeight="1">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/black"
                    >

                    <ImageView
                        android:id="@+id/ivCaluon"
                        android:layout_width="120dp"
                        android:layout_height="150dp"
                        android:layout_centerInParent="true"
                        android:src="@drawable/caluon">

                    </ImageView>

                </RelativeLayout>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_height="0dp"
                android:layout_width="0dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="0dp"
                android:layout_margin="5dp"
                app:layout_rowWeight="1"
                app:layout_columnWeight="1">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/black"
                    >

                    <ImageView
                        android:id="@+id/ivCabuou"
                        android:layout_width="120dp"
                        android:layout_height="150dp"
                        android:layout_centerInParent="true"
                        android:src="@drawable/cabuou">

                    </ImageView>


                </RelativeLayout>

            </androidx.cardview.widget.CardView>

        </androidx.gridlayout.widget.GridLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>