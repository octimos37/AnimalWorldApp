<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.ClassFragment">

    <!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout

        android:background="@color/body"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
<androidx.gridlayout.widget.GridLayout

    android:layout_width="0dp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintBottom_toBottomOf="parent"
    android:layout_margin="10dp"
    android:background="@color/body"
    android:layout_height="0dp"
    app:columnCount="2"
    app:rowCount="3">

    <androidx.cardview.widget.CardView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="5dp"
        app:cardElevation="0dp"

        app:cardCornerRadius="0dp"
        app:layout_rowWeight="1"
        app:layout_columnWeight="1">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/body"
            >

            <ImageView

                android:id="@+id/ivDongvatcovu"
                android:layout_width="120dp"
                android:layout_height="150dp"

                android:layout_centerInParent="true"
                android:src="@drawable/dong_vat_co_vu">

            </ImageView>



        </RelativeLayout>

    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:cardCornerRadius="0dp"
        app:cardElevation="0dp"
        android:layout_margin="5dp"
        app:layout_rowWeight="1"
        app:layout_columnWeight="1">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/body"
            >

            <ImageView
                android:id="@+id/ivChim"
                android:layout_width="120dp"
                android:layout_height="150dp"
                android:layout_centerInParent="true"
                android:src="@drawable/chim">

            </ImageView>

        </RelativeLayout>

    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:cardCornerRadius="0dp"
        app:cardElevation="0dp"
        android:layout_margin="5dp"
        app:layout_rowWeight="1"
        app:layout_columnWeight="1">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/body"
            >

            <ImageView
                android:id="@+id/ivBosat"
                android:layout_width="120dp"
                android:layout_height="150dp"
                android:layout_centerInParent="true"
                android:src="@drawable/bo_sat">

            </ImageView>


        </RelativeLayout>

    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:cardCornerRadius="0dp"
        app:cardElevation="0dp"
        android:layout_margin="5dp"
        app:layout_rowWeight="1"
        app:layout_columnWeight="1">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/body"
            >

            <ImageView
                android:id="@+id/ivCa"
                android:layout_width="120dp"
                android:layout_height="150dp"
                android:layout_centerInParent="true"
                android:src="@drawable/ca">

            </ImageView>

        </RelativeLayout>

    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:cardCornerRadius="0dp"
        app:cardElevation="0dp"
        android:layout_margin="5dp"
        app:layout_rowWeight="1"
        app:layout_columnWeight="1">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/body"
            >

            <ImageView
                android:id="@+id/ivLuongcu"
                android:layout_width="120dp"
                android:layout_height="150dp"
                android:layout_centerInParent="true"
                android:src="@drawable/luong_cu">

            </ImageView>

        </RelativeLayout>

    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:cardCornerRadius="0dp"
        app:cardElevation="0dp"
        android:layout_margin="5dp"
        app:layout_rowWeight="1"
        app:layout_columnWeight="1">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/body"
            >

            <ImageView
                android:id="@+id/ivKhongxuongsong"
                android:layout_width="120dp"
                android:layout_height="150dp"
                android:layout_centerInParent="true"
                android:src="@drawable/khong_xuong_song">

            </ImageView>


        </RelativeLayout>

    </androidx.cardview.widget.CardView>
</androidx.gridlayout.widget.GridLayout>
</androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>